{
  "Suffix": "api",
  "Property": {
    "AlwaysOn": true,
    "HttpsOnly": true,
    "MinTlsVersion": "1.2",
    "FtpsState": "Disabled",
    "Use32BitWorkerProcess": false,
    "Http20Enabled": true,
    "WebSocketsEnabled": true
  },
  "DockerCompose": {
    "Path": "./../.resx/be/docker-compose.yml",
    "Placeholders": [
      {
        "Name": "${__dc__IMAGE}",
        "Value": null,
        "Expression": "\"${ACRUsername}.azurecr.io/${BranchName}/thesisapi:${Tag}\""
      },
      {
        "Name": "__dc__SPRING_SERVER_PORT",
        "Value": "5000",
        "Expression": null
      },
      {
        "Name": "__dc__MYSQL_ROOT_PASSWORD",
        "Value": null,
        "Expression": "$MySqlRootPassword"
      },
      {
        "Name": "__dc__MYSQL_USERNAME",
        "Value": null,
        "Expression": "$MySqlUsername"
      },
      {
        "Name": "__dc__MYSQL_PASSWORD",
        "Value": null,
        "Expression": "$MySqlPassword"
      },
      {
        "Name": "__dc__MYSQL_DATABASE",
        "Value": "thesis",
        "Expression": null
      }
    ]
  },
  "AppSettings": [
    {
      "Name": "WEBSITES_ENABLE_APP_SERVICE_STORAGE",
      "Value": "TRUE",
      "Expression": null
    },
    {
      "Name": "DOCKER_REGISTRY_SERVER_URL",
      "Value": null,
      "Expression": "\"https://${ACRUsername}.azurecr.io\""
    },
    {
      "Name": "DOCKER_REGISTRY_SERVER_USERNAME",
      "Value": null,
      "Expression": "$ACRUsername"
    },
    {
      "Name": "DOCKER_REGISTRY_SERVER_PASSWORD",
      "Value": null,
      "Expression": "$ACRPassword"
    }
  ],
  "ConnectionStrings": []
}
